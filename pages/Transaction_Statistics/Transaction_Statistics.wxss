.container {
	padding: 20rpx;
  }
  
  .section {
	margin-top: 20px;
	display: flex;
	flex-direction: column;
	align-items: center;
	width: 100%;
  }
  
  .label {
	font-size: 16px;
	font-weight: bold;
  }
  
  .value {
	font-size: 16px;
	margin-bottom: 15px; /* 增加底部间距 */
	white-space: pre-wrap; /* 允许换行 */
	word-break: break-all;
  }
  
  .flow-chart {
	width: 100%;
	height: 350px;
	margin-top: 10px;
  }